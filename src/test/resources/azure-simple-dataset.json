{
  "name":        "AzureSimpleDataset",
  "description": "Dataset for testing dataset ingest in azure",
  "schema":      {
    "tables":        [
      {
        "name":    "participant",
        "columns": [
          {"name": "id", "datatype": "string"},
          {"name": "age", "datatype": "integer"},
          {"name": "first_name", "datatype": "string"},
          {"name": "last_name", "datatype": "string"},
          {"name": "favorite_animals", "datatype": "string", "array_of": true}
        ],
        "primaryKey": ["id"]
      }
    ]
  },
  "cloudPlatform": "azure",
  "region": "us-east1"
}
