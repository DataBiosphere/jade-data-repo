kind: Secret
metadata:
  name: google-clientid-secret
data:
  clientid_secret: {{ .Data.oauthClientSecret | base64Encode }}
  clientid: {{ .Data.oauthClientId | base64Encode }}


{{end}}
{{end}}
{{end}}
